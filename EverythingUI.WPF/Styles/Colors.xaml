<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Primary Colors -->
    <Color x:Key="PrimaryColor">#5B7FFF</Color>
    <Color x:Key="PrimaryDarkColor">#4A6BE5</Color>
    <Color x:Key="PrimaryLightColor">#7B9AFF</Color>
    
    <!-- Secondary Colors -->
    <Color x:Key="SecondaryColor">#FF6B9D</Color>
    <Color x:Key="SecondaryDarkColor">#E55A8A</Color>
    <Color x:Key="SecondaryLightColor">#FF8FB0</Color>
    
    <!-- Success, Warning, Error Colors -->
    <Color x:Key="SuccessColor">#52C41A</Color>
    <Color x:Key="WarningColor">#FAAD14</Color>
    <Color x:Key="ErrorColor">#FF4D4F</Color>
    <Color x:Key="InfoColor">#1890FF</Color>
    
    <!-- Neutral Colors -->
    <Color x:Key="Gray900">#1F2937</Color>
    <Color x:Key="Gray800">#374151</Color>
    <Color x:Key="Gray700">#4B5563</Color>
    <Color x:Key="Gray600">#6B7280</Color>
    <Color x:Key="Gray500">#9CA3AF</Color>
    <Color x:Key="Gray400">#D1D5DB</Color>
    <Color x:Key="Gray300">#E5E7EB</Color>
    <Color x:Key="Gray200">#F3F4F6</Color>
    <Color x:Key="Gray100">#F9FAFB</Color>
    <Color x:Key="WhiteColor">#FFFFFF</Color>
    <Color x:Key="BlackColor">#000000</Color>
    
    <!-- Background Colors -->
    <Color x:Key="BackgroundColor">#F5F7FA</Color>
    <Color x:Key="SurfaceColor">#FFFFFF</Color>
    <Color x:Key="CardBackgroundColor">#FFFFFF</Color>
    
    <!-- Text Colors -->
    <Color x:Key="TextPrimaryColor">#1F2937</Color>
    <Color x:Key="TextSecondaryColor">#6B7280</Color>
    <Color x:Key="TextDisabledColor">#9CA3AF</Color>
    <Color x:Key="TextInverseColor">#FFFFFF</Color>
    
    <!-- Border Colors -->
    <Color x:Key="BorderColor">#E5E7EB</Color>
    <Color x:Key="BorderHoverColor">#D1D5DB</Color>
    <Color x:Key="BorderFocusColor">#5B7FFF</Color>
    
    <!-- Brush Resources -->
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"/>
    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="{StaticResource PrimaryDarkColor}"/>
    <SolidColorBrush x:Key="PrimaryLightBrush" Color="{StaticResource PrimaryLightColor}"/>
    
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}"/>
    <SolidColorBrush x:Key="SecondaryDarkBrush" Color="{StaticResource SecondaryDarkColor}"/>
    <SolidColorBrush x:Key="SecondaryLightBrush" Color="{StaticResource SecondaryLightColor}"/>
    
    <SolidColorBrush x:Key="SuccessBrush" Color="{StaticResource SuccessColor}"/>
    <SolidColorBrush x:Key="WarningBrush" Color="{StaticResource WarningColor}"/>
    <SolidColorBrush x:Key="ErrorBrush" Color="{StaticResource ErrorColor}"/>
    <SolidColorBrush x:Key="InfoBrush" Color="{StaticResource InfoColor}"/>
    
    <SolidColorBrush x:Key="Gray900Brush" Color="{StaticResource Gray900}"/>
    <SolidColorBrush x:Key="Gray800Brush" Color="{StaticResource Gray800}"/>
    <SolidColorBrush x:Key="Gray700Brush" Color="{StaticResource Gray700}"/>
    <SolidColorBrush x:Key="Gray600Brush" Color="{StaticResource Gray600}"/>
    <SolidColorBrush x:Key="Gray500Brush" Color="{StaticResource Gray500}"/>
    <SolidColorBrush x:Key="Gray400Brush" Color="{StaticResource Gray400}"/>
    <SolidColorBrush x:Key="Gray300Brush" Color="{StaticResource Gray300}"/>
    <SolidColorBrush x:Key="Gray200Brush" Color="{StaticResource Gray200}"/>
    <SolidColorBrush x:Key="Gray100Brush" Color="{StaticResource Gray100}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource BlackColor}"/>
    
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource SurfaceColor}"/>
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="{StaticResource CardBackgroundColor}"/>
    
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource TextPrimaryColor}"/>
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource TextSecondaryColor}"/>
    <SolidColorBrush x:Key="TextDisabledBrush" Color="{StaticResource TextDisabledColor}"/>
    <SolidColorBrush x:Key="TextInverseBrush" Color="{StaticResource TextInverseColor}"/>
    
    <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource BorderColor}"/>
    <SolidColorBrush x:Key="BorderHoverBrush" Color="{StaticResource BorderHoverColor}"/>
    <SolidColorBrush x:Key="BorderFocusBrush" Color="{StaticResource BorderFocusColor}"/>
    
</ResourceDictionary>
